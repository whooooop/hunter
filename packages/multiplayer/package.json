{
  "name": "@hunter/multiplayer",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:proto && tsc",
    "build:proto": "protoc --plugin=\"./node_modules/.bin/protoc-gen-ts_proto\" --ts_proto_out=\"./src\" --ts_proto_opt=\"esModuleInterop=true,forceLong=string,outputServices=false\" --proto_path=\"./src\" ./src/sync.proto"
  },
  "license": "ISC",
  "dependencies": {
    "@bufbuild/protobuf": "2.4.0",
    "@types/ws": "^8.18.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "ts-proto": "^2.7.0",
    "typescript": "^5.4.3"
  }
}
